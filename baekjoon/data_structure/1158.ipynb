   "metadata": {},
   "outputs": [
    {
     "name": "stdin",
     "output_type": "stream",
     "text": [
      " 7 3\n"
     ]
    },
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "<3, 6, 2, 7, 5, 1, 4>\n"
     ]
    }
   ],
   "source": [
    "l, k = map(int, input().split())\n",
    "li = list(i for i in range(1, l+1))\n",
    "result = []\n",
    "idx = 0\n",
    "\n",
    "for _ in range(l):\n",
    "    idx += (k-1)\n",
    "    idx = idx % len(li)\n",
    "    result.append(li.pop(idx))\n",
    "    \n",
    "print(str(result).replace('[', '<').replace(']', '>'))"
   ]
  },
