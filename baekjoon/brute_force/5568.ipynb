   "source": [
    "from itertools import permutations\n",
    "\n",
    "n = int(input())\n",
    "k = int(input())\n",
    "li = list(input().strip() for _ in range(n))\n",
    "result = set()\n",
    "\n",
    "for num in permutations(li, k):\n",
    "    num = ''.join(num)    \n",
    "    result.add(num)\n",
    "    \n",
    "print(len(result))"
   ]
